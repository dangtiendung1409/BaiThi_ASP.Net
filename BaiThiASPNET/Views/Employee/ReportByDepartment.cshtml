@model List<BaiThiASPNET.Models.Employee>

<h1>Report and Statistics</h1>

<p>
    Number of Employees: @ViewData["NumberOfEmployees"]<br />
    Average Salary: @ViewData["AverageSalary"]
</p>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Code</th>
            <th>Rank</th>
            <!-- Thêm các cột khác nếu cần -->
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Code</td>
                <td>@employee.Rank</td>
                <!-- Thêm các cột khác nếu cần -->
            </tr>
        }
    </tbody>
</table>
